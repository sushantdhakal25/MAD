<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tabexpirementhomework">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.example.tabexpirementhomework.FirstActivity"
            android:label="FIrst Activity"
            android:parentActivityName="com.example.tabexpirementhomework.FirstActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.tabexpirementhomework.FirstActivity"/>
        </activity>

        <activity android:name="com.example.tabexpirementhomework.SecondActivity"
            android:label="Second Activity"
            android:parentActivityName="com.example.tabexpirementhomework.SecondActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.tabexpirementhomework.SecondActivity"/>
        </activity>

        <activity android:name="com.example.tabexpirementhomework.ThirdActivity"
            android:label="Third Activity"
            android:parentActivityName="com.example.tabexpirementhomework.ThirdActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.tabexpirementhomework.ThirdActivity"/>
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>